{
  "mcpProxy": {
    "baseURL": "http://localhost",
    "addr": ":9000",
    "name": "MCP Proxy",
    "version": "1.0.0",
    "options": {
      "panicIfInvalid": true,
      "logEnabled": true
    }
  },
  "mcpServers": {
    "fetch": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-fetch"
      ]
    },
    "youtube_transcript": {
      "type": "stdio",
      "command": "docker",
      "args": [
        "exec",
        "-i",
        "mcp-youtube-transcript"
      ]
    }
  }
}
